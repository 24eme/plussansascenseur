{% extends 'base.html.twig' %}

{% block body %}

<h2>Mon ascenseur</h2>
<div class="row my-4">
    <div class="col-sm-12" id="addrSearch" data-target="{{ path('listing', {coordinates: '_coordinates_', photoid: photoid}) }}">
		<input class="typeahead form-control" type="text" placeholder="{% if address %}{{ address }}{% else %}Indiquer ici l'adresse de l'ascenseur{% endif %}">
    </div>
</div>
<h2>Résultats</h2>

<table class="table table-striped table-hover">
	<tbody>
		{%  for elevator in elevators %}
		<tr>
			<td></td>
			<td></td>
		</tr>
		{% endfor %}
		<tr class="clickable-row" data-href="">
			<td></td>
			<td>Mon ascenseur n'est pas listé ici</td>
		</tr>
				
	</tbody>
</table>

{% endblock %}
